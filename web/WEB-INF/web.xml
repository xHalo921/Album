<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true"
>
    <servlet>
        <description>sign</description>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>njnu15.servlet.LoginServlet</servlet-class>
    </servlet>

    <servlet>
        <description>sign</description>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>njnu15.servlet.RegisterServlet</servlet-class>
    </servlet>

    <servlet>
        <description>sign</description>
        <servlet-name>createAlbum</servlet-name>
        <servlet-class>njnu15.servlet.createAlbum</servlet-class>
    </servlet>

    <servlet>
        <description>addCategory</description>
        <servlet-name>addCategory</servlet-name>
        <servlet-class>njnu15.servlet.addCategory</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>UpLoadPic</servlet-name>
        <servlet-class>njnu15.servlet.UpLoadPic</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>DelPhoto</servlet-name>
        <servlet-class>njnu15.servlet.DelPhoto</servlet-class>
    </servlet>

    <servlet>
        <description>sign</description>
        <servlet-name>delAlbum</servlet-name>
        <servlet-class>njnu15.servlet.delAlbum</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/njnu15/servlet/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/njnu15/servlet/RegisterServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>DelPhoto</servlet-name>
        <url-pattern>/njnu15/servlet/DelPhoto</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>createAlbum</servlet-name>
        <url-pattern>/njnu15/servlet/createAlbum</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>addCategory</servlet-name>
        <url-pattern>/njnu15/servlet/addCategory</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UpLoadPic</servlet-name>
        <url-pattern>/njnu15/servlet/UpLoadPic</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>delAlbum</servlet-name>
        <url-pattern>/njnu15/servlet/delAlbum</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>login.jsp</welcome-file>
    </welcome-file-list>
</web-app>